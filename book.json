        },
        "github-buttons": {
            "repo": "kilvn/cs-self-learning",
            "types": [
                "star",
                "watch"
            ],
            "size": "small"
        },
        "image-captions": {
            "caption": "_CAPTION_"
        },
        "anchor-navigation-ex": {
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": false,
            "mode": "float",
