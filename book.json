        },
        "github": {
            "url": "https://github.com/kilvn/cs-self-learning"
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy <a style=\"font-weight: bold\" href=\"https://www.kilvn.com/\" target=\"_blank\">Kilvn</a> 2022.",
            "modify_label": "最后更新时间：",
